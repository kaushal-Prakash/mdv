{
  "expo": {
    "name": "mdv",
    "slug": "mdv",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icon.png", 
    "scheme": "mdv",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.kaushalprakash.mdv",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "mimeType": "text/markdown"
            },
            {
              "mimeType": "text/plain"
            }
          ],
          "category": "DEFAULT"
        },{
          "action": "EDIT",
          "data": [
            {
              "mimeType": "text/markdown"
            },
            {
              "mimeType": "text/plain"
            }
          ],
          "category": "DEFAULT"
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/adaptive-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/adaptive-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "81053c12-6aa4-4c95-b153-3b3c20d28895"
      }
    }
  }
}